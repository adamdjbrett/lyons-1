<section class="page-section" id="{{ about.section }}">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <h2 class="section-heading text-uppercase">{{ about.title }}</h2>
        <h3 class="section-subheading text-muted">{{ about.text }}</h3>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-8 mx-auto text-center">
        <div class="large text-muted">{{ about.body }}</div>
      </div>
    </div>
    <div class="row mt-3">
    	{% for about_image in about.images %}
	    	<div class="col-lg-6 col-sm-12 p-3">
	    		<img src="{{ about_image.image }}" alt="{{ about_image.alt }}" 
                class="img-fluid">
	    	</div>
    	{% endfor %}
    	</div>
    </div>
</section>